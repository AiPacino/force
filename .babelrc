{
  "presets": [
    "es2015",
    "react",
    "stage-3"
  ],
  "plugins": [
    "babel-plugin-rewire",
    "inline-react-svg",
    "transform-runtime",
    ["module-resolver", {
      "root": ["./desktop"],
      "alias": {
        "desktop": "./desktop",
        "mobile": "./mobile",
        "root": "./"
      }
    }],
    "transform-class-properties"
  ]
}
